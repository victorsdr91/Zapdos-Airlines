        <servlet-class>controller.loadFlights</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>refreshBooking</servlet-name>
        <servlet-class>controller.refreshBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savePassengers</servlet-name>
        <servlet-class>controller.savePassengers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>selectSeat</servlet-name>
        <servlet-class>controller.selectSeat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>seatSave</servlet-name>
        <servlet-class>controller.seatSave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userRegister</servlet-name>
        <servlet-class>controller.userRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkEmail</servlet-name>
        <servlet-class>controller.checkEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userLogin</servlet-name>
        <servlet-class>controller.userLogin</servlet-class>
    </servlet>
    <servlet>
