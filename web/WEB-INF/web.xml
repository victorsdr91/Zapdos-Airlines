    </servlet>
    <servlet>
        <servlet-name>saveBooking</servlet-name>
        <servlet-class>controller.saveBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkFlight</servlet-name>
        <servlet-class>controller.checkFlight</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteFlight</servlet-name>
        <servlet-class>controller.deleteFlight</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getMyBookings</servlet-name>
        <servlet-class>controller.getMyBookings</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getIncidents</servlet-name>
        <servlet-class>controller.getIncidents</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dispatcher</servlet-name>
        <url-pattern>/Dispatcher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadFrom</servlet-name>
        <url-pattern>/loadFrom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadTo</servlet-name>
        <url-pattern>/loadTo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadFlights</servlet-name>
        <url-pattern>/loadFlights</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>refreshBooking</servlet-name>
        <url-pattern>/refreshBooking</url-pattern>
    </servlet-mapping>
